{
    "routes": [
        {
          "route": "/home",
          "statusCode": 301,
          "redirect": "/",
          "allowedRoles": ["anonymous", "authenticated"]
        },
        {
          "route": "/*",
          "allowedRoles": ["authenticated"]
        }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 301,
        "redirect": "/"
      }
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "trailingSlash": "auto"
  }